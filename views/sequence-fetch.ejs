    <meta property="og:description" content="Free online tool to fetch multiple nucleotide sequences from NCBI databases with batch processing and customizable export options">
    <meta property="og:type" content="website">
    <meta property="og:url" content="https://ncbi.jyotsnapriyam.com/sequence-fetch">
    
    <!-- Canonical URL -->
    <link rel="canonical" href="https://ncbi.jyotsnapriyam.com/sequence-fetch">
    
    <link rel="stylesheet" href="/styles/sequence-fetch.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600;700&display=swap" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" rel="stylesheet">
    
